<svg width="500" height="500" viewBox="0 0 500 500" xmlns="http://www.w3.org/2000/svg">
  <style>
    .flame { fill: #FF6B00; }
    .background { fill: #F5F5F5; }
    .text { fill: #333333; font-family: Arial, sans-serif; font-size: 24px; text-anchor: middle; }
    .form { fill: #FFFFFF; stroke: #CCCCCC; stroke-width: 2; }
    .button { fill: #FF6B00; }
    .button-text { fill: #FFFFFF; font-family: Arial, sans-serif; font-size: 16px; text-anchor: middle; }
    @keyframes pulse {
      0% { transform: scale(1); }
      50% { transform: scale(1.05); }
      100% { transform: scale(1); }
    }
    .animate-pulse {
      animation: pulse 2s infinite;
    }
  </style>
  <rect width="500" height="500" fill="white"/>
  
  <!-- Background -->
  <rect class="background" x="50" y="50" width="400" height="400" rx="20" ry="20"/>
  
  <!-- Form -->
  <rect class="form" x="100" y="120" width="300" height="280" rx="10" ry="10"/>
  
  <!-- Form Fields -->
  <rect x="120" y="150" width="260" height="40" rx="5" ry="5" fill="#F9F9F9" stroke="#DDDDDD"/>
  <rect x="120" y="200" width="260" height="40" rx="5" ry="5" fill="#F9F9F9" stroke="#DDDDDD"/>
  <rect x="120" y="250" width="260" height="40" rx="5" ry="5" fill="#F9F9F9" stroke="#DDDDDD"/>
  
  <!-- Button -->
  <g class="animate-pulse">
    <rect class="button" x="120" y="320" width="260" height="50" rx="25" ry="25"/>
    <text class="button-text" x="250" y="350">SIGN UP</text>
  </g>
  
  <!-- Flame Icon -->
  <g transform="translate(250, 90) scale(0.4)">
    <path class="flame" d="M0,-80 C30,-70 40,-40 30,-10 C70,-40 70,-10 50,20 C90,-10 90,30 70,60 C110,30 90,90 50,100 C60,110 20,110 0,100 C-20,110 -60,110 -50,100 C-90,90 -110,30 -70,60 C-90,30 -90,-10 -50,20 C-70,-10 -70,-40 -30,-10 C-40,-40 -30,-70 0,-80 Z" />
  </g>
  
  <text x="250" y="450" class="text">Signup Placeholder</text>
</svg>